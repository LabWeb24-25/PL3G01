<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - biblioon</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/biblioon.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <div class="grid-container">
            <div class="row1">
                <div class="grid-item">
                    <a href="#">
                    <svg width="12" height="18" xmlns="http://www.w3.org/2000/svg">
                        <path d="M6.024.138c1.664 0 3.215.72 4.32 1.964a6.405 6.405 0 011.581 4.238c0 .443-.045.882-.132 1.312l-.171-.03.162.048a10.377 10.377 0 01-1.388 2.959l-.229.318-4.163 6.582-4.16-6.568A10.349 10.349 0 01.219 7.69a6.304 6.304 0 01-.143-1.343c0-1.591.573-3.092 1.592-4.248A5.757 5.757 0 015.983.138zm0 1.35h-.04a4.407 4.407 0 00-3.306 1.505 5.055 5.055 0 00-1.253 3.354c0 .358.036.702.1 1.007a9.09 9.09 0 001.43 2.841l3.047 4.81 3.054-4.823a8.943 8.943 0 001.321-2.536l.104-.332.035-.19c.026-.172.043-.346.052-.52l.007-.264c0-1.259-.45-2.44-1.244-3.344-.85-.957-2.035-1.508-3.307-1.508zM6 3.813a2 2 0 110 4 2 2 0 010-4z" fill="#000" fill-rule="nonzero"></path>
                    </svg>
                    Localizacao
                    </a>
                </div>
                <div class="grid-item">
                    <a href="#">Informacoes</a>
                </div>
                <div class="grid-item">3</div>
                <div class="grid-item">
                    <a href="#">
                        <svg width="19" height="19" xmlns="http://www.w3.org/2000/svg">
                            <path d="M9.773.813c4.971 0 9 3.827 9 8.55 0 4.722-4.029 8.55-9 8.55-4.97 0-9-3.828-9-8.55 0-4.723 4.03-8.55 9-8.55zm.787 11.257H8.975c-1.964.004-3.554 1.515-3.558 3.38h0v.388l-.03-.018a8.311 8.311 0 004.385 1.236 8.27 8.27 0 004.345-1.212h0v-.394c-.003-1.865-1.594-3.376-3.557-3.38h0zM9.778 1.668h-.022c-4.47 0-8.094 3.442-8.094 7.689 0 2.348 1.108 4.451 2.868 5.86.14-2.219 2.076-3.977 4.445-3.98h1.584c2.37.003 4.305 1.762 4.457 3.982 1.747-1.41 2.856-3.514 2.856-5.862 0-4.247-3.623-7.69-8.094-7.69h0zm-.005 1.71c1.989 0 3.6 1.53 3.6 3.42 0 1.889-1.611 3.42-3.6 3.42-1.988 0-3.6-1.531-3.6-3.42 0-1.89 1.612-3.42 3.6-3.42zm0 .855c-1.49.003-2.696 1.15-2.7 2.565 0 1.416 1.21 2.565 2.7 2.565 1.491 0 2.7-1.149 2.7-2.565 0-1.417-1.209-2.565-2.7-2.565h0z" fill="#000" fill-rule="nonzero"></path>
                        </svg>
                        Localizacao
                        <svg width="13" height="8" xmlns="http://www.w3.org/2000/svg">
                            <path d="M1.56.344l5.075 5.192 4.84-5.18 1.096 1.024L6.66 7.708.487 1.392z" fill="#000" fill-rule="nonzero"></path>
                        </svg>
                    </a>
                </div>
                <div class="grid-item">
                    <a href="#">
                        <svg width="16" height="14" xmlns="http://www.w3.org/2000/svg">
                            <path d="M11 .143A4.654 4.654 0 007.85 1.618l-.135.153-.134-.153A4.647 4.647 0 004.214.138C1.863.138.04 1.889.04 4.188c0 .737.192 1.447.568 2.161.464.882 1.183 1.75 2.348 2.902l.257.25c.45.437 3.159 2.948 4.032 3.796l.47.456.703-.68c1.045-.998 3.384-3.169 3.8-3.571 1.318-1.278 2.108-2.208 2.605-3.153.376-.714.567-1.424.567-2.161 0-2.299-1.823-4.05-4.175-4.05l-.213.005zm.214 1.345c1.618 0 2.825 1.16 2.825 2.7 0 .507-.134 1.004-.412 1.533-.386.734-1.033 1.514-2.112 2.579l-.377.365-3.424 3.212-3.562-3.344C2.928 7.346 2.216 6.507 1.802 5.72c-.278-.529-.413-1.026-.413-1.533 0-1.54 1.208-2.7 2.825-2.7 1.242 0 2.455.782 2.874 1.826l.626 1.562.627-1.562c.419-1.044 1.63-1.826 2.873-1.826z" fill="#000" fill-rule="nonzero"></path>
                        </svg>
                        Historico
                    </a>
                </div>
            </div>

            <div class="row2">
                <div class="grid-item">1</div>
                <div class="grid-item">2</div>
                <div class="grid-item">3</div>
                <div class="grid-item">4</div>
                <div class="grid-item">5</div>
            </div>

            <div class="row3">
                <div class="grid-item">
                    <a href="#">Livros</a>
                </div>
                <div class="grid-item">
                    <a href="#">Ciencias Exatas</a>
                </div>
                <div class="grid-item">
                    <a href="#">Ciencias Naturais</a>
                </div>
                <div class="grid-item">
                    <a href="#">Ciencias Tecnologicas</a>
                </div>
                <div class="grid-item">
                    <a href="#">Ciencias Agrarias</a>
                </div>
                <div class="grid-item">
                    <a href="#">Ciencias Sociais</a>
                </div>
                <div class="grid-item">
                    <a href="#">Ciencias Da Saude</a>
                </div>
                <div class="grid-item">
                    <a href="#">Ficcao</a>
                </div>
                <div class="grid-item">
                    <a href="#">Nao Ficcao</a>
                </div>
            </div>
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="row4">
            <div class="grid-item">1</div>
            <div class="grid-item">2</div>
            <div class="grid-item">3</div>
            <div class="grid-item">4</div>
            <div class="grid-item">5</div>
            <div class="grid-item">6</div>
            <div class="grid-item">7</div>
            <div class="grid-item">8</div>
            <div class="grid-item">9</div>
            <div class="grid-item">10</div>
            <div class="grid-item">11</div>
            <div class="grid-item">12</div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
